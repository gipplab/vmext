                fieldset.form-group.option-type
                  legend MIME Type
                  .form-check
                    label.form-check-label
                      <input class='form-check-input option-json' type='radio', name='type', value='json' /> application/json
                  .form-check
                    label.form-check-label
                      <input class='form-check-input option-svg' type='radio', name='type', value='svg' checked/> image/svg+xml
                fieldset.form-group.option-type
                  legend Advanced Options
                  .form-check
                    label.form-check-label
                      <input class='form-check-input option-renderFormula' type='checkbox', name='type', value='svg' checked/> Render Formula
                  .form-check
                    label.form-check-label
                      <input class='form-check-input option-collapseOneChildNodes' type='checkbox', name='type', value='svg'/> collapse nodes with only 1 child
                input.btn.btn-success(type='submit', value='Render')
                a(href='/api/docs') open API specs
            .col-md-8.abstract-syntax-tree
              div.renderedAST.form-control

              script(src='../js/index.js', type='text/javascript')
